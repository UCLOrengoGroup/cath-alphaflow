
executor {
    name = 'sge'
    queueSize = 10
}

process {    
        executor = 'sge'
        beforeScript = "source ${launchDir}/platforms/ucl_myriad/source"
        cpus = 1
        memory = '4 GB'
        time = '10min'
        clusterOptions = '-S /bin/bash\n#$ -l tmem=4G -N cath_job'
    }
}

